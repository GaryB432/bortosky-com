{
  "name": "bortosky-com",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "nx exec -- vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "nx exec -- vitest --run",
    "lint": "nx exec -- eslint ."
  },
  "devDependencies": {
    "@fontsource/montserrat": "^5.0.13",
    "@gb-nx/svelte": "7.0.1",
    "@gb-nx/workspace": "^5.0.0",
    "@sveltejs/adapter-vercel": "^4.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/cookie": "^0.5.1",
    "@types/cytoscape": "^3.19.12",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-svelte": "^2.36.0",
    "json-schema-to-typescript": "^13.1.1",
    "nx": "17.2.8",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.69.3",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "the-new-css-reset": "^1.11.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "cytoscape": "^3.26.0",
    "tippy.js": "^6.3.7"
  },
  "type": "module",
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.svelte-kit",
          "{projectRoot}/.vercel",
          "{projectRoot}/build"
        ]
      },
      "test": {
        "outputs": [
          "{projectRoot}/coverage",
          "{projectRoot}/junit"
        ]
      }
    }
  },
  "bugs": "https://github.com/GaryB432/bortosky-com/issues"
}
